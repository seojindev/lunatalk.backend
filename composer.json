{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "doctrine/dbal": "^3.1",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.40",
        "laravel/passport": "^10.1",
        "laravel/slack-notification-channel": "^2.3",
        "laravel/tinker": "^2.5",
      "ext-dom": "*",
      "ext-openssl": "*"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3",
        "spatie/phpunit-watcher": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "if [[ $COMPOSER_DEV_MODE -eq 1 ]]; then php artisan ide-helper:generate; fi",
            "if [[ $COMPOSER_DEV_MODE -eq 1 ]]; then php artisan ide-helper:models -W; fi",
            "if [[ $COMPOSER_DEV_MODE -eq 1 ]]; then php artisan ide-helper:meta; fi",
            "php artisan optimize"
        ],
        "app-reset": [
            "composer dump-autoload",
            "php artisan optimize:clear",
            "php artisan migrate:refresh --seed"
        ],
        "app-clear": [
            "composer dump-autoload",
            "if [[ $COMPOSER_DEV_MODE -eq 1 ]]; then php artisan ide-helper:generate; fi",
            "if [[ $COMPOSER_DEV_MODE -eq 1 ]]; then php artisan ide-helper:models -W; fi",
            "if [[ $COMPOSER_DEV_MODE -eq 1 ]]; then php artisan ide-helper:meta; fi",
            "php artisan optimize:clear"
        ],
        "unit-test:watch": [
            "phpunit-watcher watch < /dev/tty"
        ],
        "unit-test:clear": [
            "php artisan config:clear --env=testing"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
